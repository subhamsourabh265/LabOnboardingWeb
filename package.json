{
  "name": "lab-onboarding",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "format:new": "npx ng lint --fix && npx pretty-quick --staged",
    "format:all": "npx ng lint --fix && npx prettier --write */**/*.*",
    "test": "ng test",
    "test:ci": "ng test --no-watch --no-progress --browsers ChromeHeadlessCI",
    "test:ci:coverage": "ng test --no-watch --no-progress --browsers ChromeHeadlessCI --code-coverage",
    "security": "npm audit --production",
    "scully": "npx scully --",
    "scully:serve": "npx scully serve --",
    "prepare": "is-ci || husky install",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/cdk": "^13.3.7",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "~13.3.0",
    "@angular/material": "^13.3.2",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/platform-server": "^12.2.16",
    "@angular/router": "~13.3.0",
    "@geneicd/fhir-library": "^1.7.12",
    "@scullyio/init": "^2.1.29",
    "@scullyio/ng-lib": "^2.1.0",
    "@scullyio/platform-server": "^2.1.0",
    "@scullyio/scully": "^2.1.0",
    "@scullyio/scully-plugin-puppeteer": "^2.1.29",
    "amazon-cognito-identity-js": "^5.2.8",
    "fast-xml-parser": "^4.0.7",
    "is-ci": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "ng-mocks": "^13.5.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.2",
    "@angular-eslint/builder": "13.2.0",
    "@angular-eslint/eslint-plugin": "13.2.0",
    "@angular-eslint/eslint-plugin-template": "13.2.0",
    "@angular-eslint/schematics": "13.2.0",
    "@angular-eslint/template-parser": "13.2.0",
    "@angular/cli": "~13.3.2",
    "@angular/compiler-cli": "~13.3.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.20.52",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "typescript": "~4.6.2"
  }
}
